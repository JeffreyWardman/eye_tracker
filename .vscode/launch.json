{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Main",
      "type": "debugpy",
      "request": "launch",
      "module": "eye_tracker.main",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}"
    }
  ]
}
